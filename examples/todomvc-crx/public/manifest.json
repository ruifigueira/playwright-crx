{
  "name": "Playwright CRX - TodoMVC",
  "version": "0.11.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Playwright CRX - TodoMVC"
  },
  "permissions": ["debugger", "tabs"],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'"
  }
}
